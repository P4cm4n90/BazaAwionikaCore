@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width = device-width, initial-scale = 1.0">


    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/w3.css" />
    @RenderSection("styles", false) ;
</head>
<body>
    <div class="tab">
        <a href="@Url.Action("Index", "Aircrafts")">Samoloty</a>
        <button class="tablinks" onclick="location.href='@Url.Action("Index", "AircraftMaintenance")'">Prace</button>
        <button class="tablinks" onclick="location.href='@Url.Action("Index", "AircraftBiuletin")'">Biuletyny</button>
        <button class="tablinks" onclick="location.href='@Url.Action("Index", "AircraftStatus")'">Baza statusów</button>
    </div>

    @RenderBody()

    <hr />
    <script type='text/javascript' src="~/lib/jquery/dist/jquery.js"></script>
    <script type='text/javascript' src="~/lib/bootstrap/disc/js/bootstrap.js"> </script>
    @RenderSection("scripts", required: false)

</body>
</html>
